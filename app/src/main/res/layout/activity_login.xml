<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="fill_parent"
                android:background="@drawable/loginbj"
                tools:context=".LoginActivity"
                android:id="@+id/login_layout"
    >
    <RelativeLayout
        android:id="@+id/login_top"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_alignParentTop="true"
        android:background="@drawable/oa_top"
        >
        <ImageView
            android:layout_width="163dp"
            android:layout_height="40dp"
            android:background="@drawable/top_logo"
            android:layout_centerInParent="true"
            />
        <Button
            android:id="@+id/login_setip"
            android:layout_width="60dp"
            android:layout_height="35dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="5dp"
            android:text="设置"
            android:textColor="@color/white"
            android:textSize="18sp"
            android:background="@drawable/search_bg"
            android:layout_centerHorizontal="true"
            />
    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/login_oa_title"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/login_top"
        android:layout_above="@+id/login_linearLayout1"
        >
        <ImageView
            android:id="@+id/oalogin_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/oalogin_title"
            android:layout_centerInParent="true"
            />
    </RelativeLayout>
    <LinearLayout
        android:id="@+id/login_linearLayout1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/main1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20sp"
            android:layout_marginRight="20sp"
            android:background="@drawable/log_content1" >

            <ImageView
                android:id="@+id/login_img_usrn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="15sp"
                android:background="@drawable/login_img_usrn"
                />

            <ImageView
                android:id="@+id/login_img_div1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="5dp"
                android:background="@drawable/login_image" />

            <EditText
                android:id="@+id/login_edt_username"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="5sp"
                android:layout_marginRight="15sp"
                android:background="@null"
                android:hint="@string/login_text_usn"
                android:singleLine="true"
                android:textColorHint="@color/gray"
                android:textSize="20sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/main2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10sp"
            android:layout_marginLeft="20sp"
            android:layout_marginRight="20sp"
            android:background="@drawable/log_content2" >

            <TextView
                android:id="@+id/login_img_pwd"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="15sp"
                android:background="@drawable/login_img_pwd"
                />

            <ImageView
                android:id="@+id/login_img_div2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="5dp"
                android:background="@drawable/login_image" />

            <EditText
                android:id="@+id/login_edt_pwd"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="5sp"
                android:layout_marginRight="15sp"
                android:background="@null"
                android:password="true"
                android:hint="@string/login_text_pwd"
                android:textColorHint="@color/gray"
                android:singleLine="true"
                android:textSize="20sp" />
        </LinearLayout>

        <RelativeLayout
            android:id="@+id/main3"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="20sp"
            android:layout_marginLeft="20sp"
            android:layout_marginRight="20sp"
            >
            <RelativeLayout
                android:id="@+id/main3_left"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toLeftOf="@+id/main3_center"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                >
                <CheckBox
                    android:id="@+id/login_rememberPassword"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:button="@drawable/login_ischeck"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="记住密码"
                    android:textColor="@color/black"
                    android:textSize="16sp"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/login_rememberPassword"
                    android:layout_marginLeft="5dp"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/main3_center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                >
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/main3_right"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/main3_center"
                android:layout_centerVertical="true"
                >
                <CheckBox
                    android:id="@+id/login_AutoLogin"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:button="@drawable/login_ischeck"
                    android:layout_centerVertical="true"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="自动登录"
                    android:textColor="@color/black"
                    android:textSize="16sp"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@+id/login_AutoLogin"
                    android:layout_marginLeft="5dp"
                    />
            </RelativeLayout>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/login_img_logbtn_background"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            >
            <Button
                android:id="@+id/login_img_logbtn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/login_btn"
                android:textColor="@color/white"
                android:layout_centerHorizontal="true"
                />

        </RelativeLayout>

    </LinearLayout>
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/login_linearLayout1"
        >
        <TextView
            android:id="@+id/login_visits"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:textColor="@color/dark_gray"
            android:textSize="16sp"
            android:layout_centerInParent="true"
            />
    </RelativeLayout>
</RelativeLayout>

