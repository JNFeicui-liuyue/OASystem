<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.hasee.oasystem.ShqglActivity">

    <RelativeLayout
        android:id="@+id/qjsp_header_rl1"
        android:layout_width="fill_parent"
        android:layout_height="45dp"
        android:layout_alignParentTop="true"
        android:background="@drawable/oa_top"
        >
        <ImageButton
            android:id="@+id/back"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:background="@drawable/search_button"
            android:src="@drawable/oa_back"
            android:layout_alignParentTop="true"/>
        <ImageView
            android:id="@+id/qjsp_header_center_imgview1"
            android:layout_width="163dp"
            android:layout_height="40dp"
            android:background="@drawable/top_logo"
            android:layout_centerInParent="true"
            />
    </RelativeLayout>

    <android.support.v4.widget.Space
        android:layout_width="match_parent"
        android:layout_height="50dp"/>

    <ImageView
        android:layout_gravity="center"
        android:src="@drawable/sqgl_bgsq"
        android:id="@+id/iv_bangongshenqing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <RelativeLayout
        android:background="@drawable/doc_search_bj"
        android:layout_gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <GridLayout
            android:id="@+id/sqgl_gridlayout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginTop="25dp"
            android:rowCount="3"
            android:columnCount="2"
            >

            <ImageButton
                android:id="@+id/sqgl_hysq_imgbtn"
                android:layout_width="106dp"
                android:layout_height="92dp"
                android:layout_column="0"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:layout_row="0"
                android:background="@drawable/oa_hysq" />

            <ImageButton
                android:id="@+id/sqgl_bxsq_imgbtn"
                android:layout_width="154dp"
                android:layout_height="92dp"
                android:layout_column="1"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:layout_row="0"
                android:background="@drawable/oa_bxsq" />

            <ImageButton
                android:id="@+id/sqgl_ycsq_imgbtn"
                android:layout_width="106dp"
                android:layout_height="92dp"
                android:layout_column="0"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:layout_row="1"
                android:visibility="visible"
                android:background="@drawable/oa_fwsq" />

            <ImageButton
                android:id="@+id/sqgl_jbsq_imgbtn"
                android:layout_width="154dp"
                android:layout_height="92dp"
                android:layout_column="1"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:layout_row="1"
                android:background="@drawable/oa_jbsq" />

            <ImageButton
                android:id="@+id/sqgl_jksq_imgbtn"
                android:layout_width="106dp"
                android:layout_height="92dp"
                android:layout_column="0"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:layout_row="2"
                android:background="@drawable/oa_jksq" />

            <ImageButton
                android:id="@+id/sqgl_qjsq_imgbtn"
                android:layout_width="154dp"
                android:layout_height="92dp"
                android:layout_column="1"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:layout_row="2"
                android:background="@drawable/oa_qjsq" />

        </GridLayout>
    </RelativeLayout>


</LinearLayout>
